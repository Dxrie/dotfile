{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0a090c90",
                "editor.background": "#0a090c90",
                "editor.foreground": "#c1c1c2",
                "text": "#c1c1c2",
                "text.muted": "#c1c1c270",
                "text.ignored": "#c1c1c240",
                "text.placeholder": "#c1c1c250",
                "ignored": "#c1c1c230",
                "element.hover": "#c1c1c230",
                "ghost_element.hover": "#A04F5F30",
                "ghost_element.selected": "#A04F5F30",
                "ghost_element.active": "#A04F5F60",
                "border": "#0a090c",
                "editor.highlighted_line.background": "#A04F5F10",
                "editor.active_line.background": "#A04F5F10",
                "panel.background": "#0a090c90",
                "title_bar.background": "#0a090c90",
                "title_bar.inactive_background": "#0a090c90",
                "status_bar.background": "#0a090c90",
                "drop_target.background": "#0a090c90",
                "elevated_surface.background": "#0a090c",
                "toolbar.background": "#0a090c90",
                "tab_bar.background": "#0a090c90",
                "tab.inactive_background": "#0a090c90",
                "tab.active_background": "#A04F5F30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0a090c",
                "scrollbar.thumb.background": "#A04F5F",
                "editor.gutter.background": "#0a090c90",
                "terminal.background": "#0a090c10",
                "terminal.foreground": "#c1c1c2",
                "terminal.dim_foreground": "#c1c1c2",
                "terminal.bright_foreground": "#c1c1c2",
                "terminal.ansi.black": "#0a090c",
                "terminal.ansi.red": "#A04F5F",
                "terminal.ansi.green": "#3D6F87",
                "terminal.ansi.yellow": "#56758C",
                "terminal.ansi.blue": "#AD758A",
                "terminal.ansi.magenta": "#6B90A8",
                "terminal.ansi.cyan": "#7BACC7",
                "terminal.ansi.white": "#c1c1c2",
                "terminal.ansi.bright_black": "#565669",
                "terminal.ansi.bright_red": "#A04F5F",
                "terminal.ansi.bright_green": "#3D6F87",
                "terminal.ansi.bright_yellow": "#56758C",
                "terminal.ansi.bright_blue": "#AD758A",
                "terminal.ansi.bright_magenta": "#6B90A8",
                "terminal.ansi.bright_cyan": "#7BACC7",
                "terminal.ansi.bright_white": "#c1c1c2",
                "modified": "#A04F5F",
                "syntax": {
                    "attribute": {
                        "color": "#A04F5F"
                    },
                    "boolean": {
                      "color": "#3D6F87"
                    },
                    "comment": {
                      "color": "#565669"
                    },
                    "comment.doc": {
                      "color": "#565669"
                    },
                    "constant": {
                      "color": "#3D6F87"
                    },
                    "function": {
                      "color": "#7BACC7"
                    },
                    "keyword": {
                      "color": "#AD758A"
                    },
                    "number": {
                      "color": "#6B90A8"
                    },
                    "operator": {
                      "color": "#AD758A"
                    },
                    "string": {
                      "color": "#A04F5F"
                    },
                    "variable": {
                      "color": "#3D6F87"
                    }
                },
                "players": [
                  {
                    "cursor": "#A04F5F",
                    "background": "#0a090c",
                    "selection": "#A04F5F30"
                  }
                ]
            }
        }
    ]
}
