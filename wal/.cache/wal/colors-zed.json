{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0f202490",
                "editor.background": "#0f202490",
                "editor.foreground": "#c3c7c8",
                "text": "#c3c7c8",
                "text.muted": "#c3c7c870",
                "text.ignored": "#c3c7c840",
                "text.placeholder": "#c3c7c850",
                "ignored": "#c3c7c830",
                "element.hover": "#c3c7c830",
                "ghost_element.hover": "#B26E4E30",
                "ghost_element.selected": "#B26E4E30",
                "ghost_element.active": "#B26E4E60",
                "border": "#0f2024",
                "editor.highlighted_line.background": "#B26E4E10",
                "editor.active_line.background": "#B26E4E10",
                "panel.background": "#0f202490",
                "title_bar.background": "#0f202490",
                "title_bar.inactive_background": "#0f202490",
                "status_bar.background": "#0f202490",
                "drop_target.background": "#0f202490",
                "elevated_surface.background": "#0f2024",
                "toolbar.background": "#0f202490",
                "tab_bar.background": "#0f202490",
                "tab.inactive_background": "#0f202490",
                "tab.active_background": "#B26E4E30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0f2024",
                "scrollbar.thumb.background": "#B26E4E",
                "editor.gutter.background": "#0f202490",
                "terminal.background": "#0f202410",
                "terminal.foreground": "#c3c7c8",
                "terminal.dim_foreground": "#c3c7c8",
                "terminal.bright_foreground": "#c3c7c8",
                "terminal.ansi.black": "#0f2024",
                "terminal.ansi.red": "#B26E4E",
                "terminal.ansi.green": "#2F8379",
                "terminal.ansi.yellow": "#608A7A",
                "terminal.ansi.blue": "#AA9473",
                "terminal.ansi.magenta": "#FA8F44",
                "terminal.ansi.cyan": "#E5965F",
                "terminal.ansi.white": "#c3c7c8",
                "terminal.ansi.bright_black": "#5e7074",
                "terminal.ansi.bright_red": "#B26E4E",
                "terminal.ansi.bright_green": "#2F8379",
                "terminal.ansi.bright_yellow": "#608A7A",
                "terminal.ansi.bright_blue": "#AA9473",
                "terminal.ansi.bright_magenta": "#FA8F44",
                "terminal.ansi.bright_cyan": "#E5965F",
                "terminal.ansi.bright_white": "#c3c7c8",
                "modified": "#B26E4E",
                "syntax": {
                    "attribute": {
                        "color": "#B26E4E"
                    },
                    "boolean": {
                      "color": "#2F8379"
                    },
                    "comment": {
                      "color": "#5e7074"
                    },
                    "comment.doc": {
                      "color": "#5e7074"
                    },
                    "constant": {
                      "color": "#2F8379"
                    },
                    "function": {
                      "color": "#E5965F"
                    },
                    "keyword": {
                      "color": "#AA9473"
                    },
                    "number": {
                      "color": "#FA8F44"
                    },
                    "operator": {
                      "color": "#AA9473"
                    },
                    "string": {
                      "color": "#B26E4E"
                    },
                    "variable": {
                      "color": "#2F8379"
                    }
                },
                "players": [
                  {
                    "cursor": "#B26E4E",
                    "background": "#0f2024",
                    "selection": "#B26E4E30"
                  }
                ]
            }
        }
    ]
}
