{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#141c3190",
                "editor.background": "#141c3190",
                "editor.foreground": "#c4c6cb",
                "text": "#c4c6cb",
                "text.muted": "#c4c6cb70",
                "text.ignored": "#c4c6cb40",
                "text.placeholder": "#c4c6cb50",
                "ignored": "#c4c6cb30",
                "element.hover": "#c4c6cb30",
                "ghost_element.hover": "#C6B97830",
                "ghost_element.selected": "#C6B97830",
                "ghost_element.active": "#C6B97860",
                "border": "#141c31",
                "editor.highlighted_line.background": "#C6B97810",
                "editor.active_line.background": "#C6B97810",
                "panel.background": "#141c3190",
                "title_bar.background": "#141c3190",
                "title_bar.inactive_background": "#141c3190",
                "status_bar.background": "#141c3190",
                "drop_target.background": "#141c3190",
                "elevated_surface.background": "#141c31",
                "toolbar.background": "#141c3190",
                "tab_bar.background": "#141c3190",
                "tab.inactive_background": "#141c3190",
                "tab.active_background": "#C6B97830",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#141c31",
                "scrollbar.thumb.background": "#C6B978",
                "editor.gutter.background": "#141c3190",
                "terminal.background": "#141c3110",
                "terminal.foreground": "#c4c6cb",
                "terminal.dim_foreground": "#c4c6cb",
                "terminal.bright_foreground": "#c4c6cb",
                "terminal.ansi.black": "#141c31",
                "terminal.ansi.red": "#C6B978",
                "terminal.ansi.green": "#546D8B",
                "terminal.ansi.yellow": "#A26FBC",
                "terminal.ansi.blue": "#649390",
                "terminal.ansi.magenta": "#51AAA9",
                "terminal.ansi.cyan": "#B4A8BC",
                "terminal.ansi.white": "#c4c6cb",
                "terminal.ansi.bright_black": "#646a7a",
                "terminal.ansi.bright_red": "#C6B978",
                "terminal.ansi.bright_green": "#546D8B",
                "terminal.ansi.bright_yellow": "#A26FBC",
                "terminal.ansi.bright_blue": "#649390",
                "terminal.ansi.bright_magenta": "#51AAA9",
                "terminal.ansi.bright_cyan": "#B4A8BC",
                "terminal.ansi.bright_white": "#c4c6cb",
                "modified": "#C6B978",
                "syntax": {
                    "attribute": {
                        "color": "#C6B978"
                    },
                    "boolean": {
                      "color": "#546D8B"
                    },
                    "comment": {
                      "color": "#646a7a"
                    },
                    "comment.doc": {
                      "color": "#646a7a"
                    },
                    "constant": {
                      "color": "#546D8B"
                    },
                    "function": {
                      "color": "#B4A8BC"
                    },
                    "keyword": {
                      "color": "#649390"
                    },
                    "number": {
                      "color": "#51AAA9"
                    },
                    "operator": {
                      "color": "#649390"
                    },
                    "string": {
                      "color": "#C6B978"
                    },
                    "variable": {
                      "color": "#546D8B"
                    }
                },
                "players": [
                  {
                    "cursor": "#C6B978",
                    "background": "#141c31",
                    "selection": "#C6B97830"
                  }
                ]
            }
        }
    ]
}
