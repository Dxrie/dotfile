{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0e142090",
                "editor.background": "#0e142090",
                "editor.foreground": "#c2c4c7",
                "text": "#c2c4c7",
                "text.muted": "#c2c4c770",
                "text.ignored": "#c2c4c740",
                "text.placeholder": "#c2c4c750",
                "ignored": "#c2c4c730",
                "element.hover": "#c2c4c730",
                "ghost_element.hover": "#A25C6230",
                "ghost_element.selected": "#A25C6230",
                "ghost_element.active": "#A25C6260",
                "border": "#0e1420",
                "editor.highlighted_line.background": "#A25C6210",
                "editor.active_line.background": "#A25C6210",
                "panel.background": "#0e142090",
                "title_bar.background": "#0e142090",
                "title_bar.inactive_background": "#0e142090",
                "status_bar.background": "#0e142090",
                "drop_target.background": "#0e142090",
                "elevated_surface.background": "#0e1420",
                "toolbar.background": "#0e142090",
                "tab_bar.background": "#0e142090",
                "tab.inactive_background": "#0e142090",
                "tab.active_background": "#A25C6230",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0e1420",
                "scrollbar.thumb.background": "#A25C62",
                "editor.gutter.background": "#0e142090",
                "terminal.background": "#0e142010",
                "terminal.foreground": "#c2c4c7",
                "terminal.dim_foreground": "#c2c4c7",
                "terminal.bright_foreground": "#c2c4c7",
                "terminal.ansi.black": "#0e1420",
                "terminal.ansi.red": "#A25C62",
                "terminal.ansi.green": "#B29B74",
                "terminal.ansi.yellow": "#697689",
                "terminal.ansi.blue": "#9C7D85",
                "terminal.ansi.magenta": "#788391",
                "terminal.ansi.cyan": "#9B9FA7",
                "terminal.ansi.white": "#c2c4c7",
                "terminal.ansi.bright_black": "#5d6472",
                "terminal.ansi.bright_red": "#A25C62",
                "terminal.ansi.bright_green": "#B29B74",
                "terminal.ansi.bright_yellow": "#697689",
                "terminal.ansi.bright_blue": "#9C7D85",
                "terminal.ansi.bright_magenta": "#788391",
                "terminal.ansi.bright_cyan": "#9B9FA7",
                "terminal.ansi.bright_white": "#c2c4c7",
                "modified": "#A25C62",
                "syntax": {
                    "attribute": {
                        "color": "#A25C62"
                    },
                    "boolean": {
                      "color": "#B29B74"
                    },
                    "comment": {
                      "color": "#5d6472"
                    },
                    "comment.doc": {
                      "color": "#5d6472"
                    },
                    "constant": {
                      "color": "#B29B74"
                    },
                    "function": {
                      "color": "#9B9FA7"
                    },
                    "keyword": {
                      "color": "#9C7D85"
                    },
                    "number": {
                      "color": "#788391"
                    },
                    "operator": {
                      "color": "#9C7D85"
                    },
                    "string": {
                      "color": "#A25C62"
                    },
                    "variable": {
                      "color": "#B29B74"
                    }
                },
                "players": [
                  {
                    "cursor": "#A25C62",
                    "background": "#0e1420",
                    "selection": "#A25C6230"
                  }
                ]
            }
        }
    ]
}
