{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#242d1e90",
                "editor.background": "#242d1e90",
                "editor.foreground": "#c8cac6",
                "text": "#c8cac6",
                "text.muted": "#c8cac670",
                "text.ignored": "#c8cac640",
                "text.placeholder": "#c8cac650",
                "ignored": "#c8cac630",
                "element.hover": "#c8cac630",
                "ghost_element.hover": "#A5C25D30",
                "ghost_element.selected": "#A5C25D30",
                "ghost_element.active": "#A5C25D60",
                "border": "#242d1e",
                "editor.highlighted_line.background": "#A5C25D10",
                "editor.active_line.background": "#A5C25D10",
                "panel.background": "#242d1e90",
                "title_bar.background": "#242d1e90",
                "title_bar.inactive_background": "#242d1e90",
                "status_bar.background": "#242d1e90",
                "drop_target.background": "#242d1e90",
                "elevated_surface.background": "#242d1e",
                "toolbar.background": "#242d1e90",
                "tab_bar.background": "#242d1e90",
                "tab.inactive_background": "#242d1e90",
                "tab.active_background": "#A5C25D30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#242d1e",
                "scrollbar.thumb.background": "#A5C25D",
                "editor.gutter.background": "#242d1e90",
                "terminal.background": "#242d1e10",
                "terminal.foreground": "#c8cac6",
                "terminal.dim_foreground": "#c8cac6",
                "terminal.bright_foreground": "#c8cac6",
                "terminal.ansi.black": "#242d1e",
                "terminal.ansi.red": "#A5C25D",
                "terminal.ansi.green": "#467687",
                "terminal.ansi.yellow": "#3CA7BF",
                "terminal.ansi.blue": "#57949D",
                "terminal.ansi.magenta": "#A7AC97",
                "terminal.ansi.cyan": "#B8C0AC",
                "terminal.ansi.white": "#c8cac6",
                "terminal.ansi.bright_black": "#6e7c65",
                "terminal.ansi.bright_red": "#A5C25D",
                "terminal.ansi.bright_green": "#467687",
                "terminal.ansi.bright_yellow": "#3CA7BF",
                "terminal.ansi.bright_blue": "#57949D",
                "terminal.ansi.bright_magenta": "#A7AC97",
                "terminal.ansi.bright_cyan": "#B8C0AC",
                "terminal.ansi.bright_white": "#c8cac6",
                "modified": "#A5C25D",
                "syntax": {
                    "attribute": {
                        "color": "#A5C25D"
                    },
                    "boolean": {
                      "color": "#467687"
                    },
                    "comment": {
                      "color": "#6e7c65"
                    },
                    "comment.doc": {
                      "color": "#6e7c65"
                    },
                    "constant": {
                      "color": "#467687"
                    },
                    "function": {
                      "color": "#B8C0AC"
                    },
                    "keyword": {
                      "color": "#57949D"
                    },
                    "number": {
                      "color": "#A7AC97"
                    },
                    "operator": {
                      "color": "#57949D"
                    },
                    "string": {
                      "color": "#A5C25D"
                    },
                    "variable": {
                      "color": "#467687"
                    }
                },
                "players": [
                  {
                    "cursor": "#A5C25D",
                    "background": "#242d1e",
                    "selection": "#A5C25D30"
                  }
                ]
            }
        }
    ]
}
