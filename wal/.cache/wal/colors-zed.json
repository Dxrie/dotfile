{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0F0F0490",
                "editor.background": "#0F0F0490",
                "editor.foreground": "#c3c3c0",
                "text": "#c3c3c0",
                "text.muted": "#c3c3c070",
                "text.ignored": "#c3c3c040",
                "text.placeholder": "#c3c3c050",
                "ignored": "#c3c3c030",
                "element.hover": "#c3c3c030",
                "ghost_element.hover": "#635F4E30",
                "ghost_element.selected": "#635F4E30",
                "ghost_element.active": "#635F4E60",
                "border": "#0F0F04",
                "editor.highlighted_line.background": "#635F4E10",
                "editor.active_line.background": "#635F4E10",
                "panel.background": "#0F0F0490",
                "title_bar.background": "#0F0F0490",
                "title_bar.inactive_background": "#0F0F0490",
                "status_bar.background": "#0F0F0490",
                "drop_target.background": "#0F0F0490",
                "elevated_surface.background": "#0F0F04",
                "toolbar.background": "#0F0F0490",
                "tab_bar.background": "#0F0F0490",
                "tab.inactive_background": "#0F0F0490",
                "tab.active_background": "#635F4E30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0F0F04",
                "scrollbar.thumb.background": "#635F4E",
                "editor.gutter.background": "#0F0F0490",
                "terminal.background": "#0F0F0410",
                "terminal.foreground": "#c3c3c0",
                "terminal.dim_foreground": "#c3c3c0",
                "terminal.bright_foreground": "#c3c3c0",
                "terminal.ansi.black": "#0F0F04",
                "terminal.ansi.red": "#635F4E",
                "terminal.ansi.green": "#906733",
                "terminal.ansi.yellow": "#C76A38",
                "terminal.ansi.blue": "#90714D",
                "terminal.ansi.magenta": "#6E8A39",
                "terminal.ansi.cyan": "#A6915E",
                "terminal.ansi.white": "#c3c3c0",
                "terminal.ansi.bright_black": "#686855",
                "terminal.ansi.bright_red": "#635F4E",
                "terminal.ansi.bright_green": "#906733",
                "terminal.ansi.bright_yellow": "#C76A38",
                "terminal.ansi.bright_blue": "#90714D",
                "terminal.ansi.bright_magenta": "#6E8A39",
                "terminal.ansi.bright_cyan": "#A6915E",
                "terminal.ansi.bright_white": "#c3c3c0",
                "modified": "#635F4E",
                "syntax": {
                    "attribute": {
                        "color": "#635F4E"
                    },
                    "boolean": {
                      "color": "#906733"
                    },
                    "comment": {
                      "color": "#686855"
                    },
                    "comment.doc": {
                      "color": "#686855"
                    },
                    "constant": {
                      "color": "#906733"
                    },
                    "function": {
                      "color": "#A6915E"
                    },
                    "keyword": {
                      "color": "#90714D"
                    },
                    "number": {
                      "color": "#6E8A39"
                    },
                    "operator": {
                      "color": "#90714D"
                    },
                    "string": {
                      "color": "#635F4E"
                    },
                    "variable": {
                      "color": "#906733"
                    }
                },
                "players": [
                  {
                    "cursor": "#635F4E",
                    "background": "#0F0F04",
                    "selection": "#635F4E30"
                  }
                ]
            }
        }
    ]
}
