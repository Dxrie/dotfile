{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#08143090",
                "editor.background": "#08143090",
                "editor.foreground": "#c1c4cb",
                "text": "#c1c4cb",
                "text.muted": "#c1c4cb70",
                "text.ignored": "#c1c4cb40",
                "text.placeholder": "#c1c4cb50",
                "ignored": "#c1c4cb30",
                "element.hover": "#c1c4cb30",
                "ghost_element.hover": "#9375B530",
                "ghost_element.selected": "#9375B530",
                "ghost_element.active": "#9375B560",
                "border": "#081430",
                "editor.highlighted_line.background": "#9375B510",
                "editor.active_line.background": "#9375B510",
                "panel.background": "#08143090",
                "title_bar.background": "#08143090",
                "title_bar.inactive_background": "#08143090",
                "status_bar.background": "#08143090",
                "drop_target.background": "#08143090",
                "elevated_surface.background": "#081430",
                "toolbar.background": "#08143090",
                "tab_bar.background": "#08143090",
                "tab.inactive_background": "#08143090",
                "tab.active_background": "#9375B530",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#081430",
                "scrollbar.thumb.background": "#9375B5",
                "editor.gutter.background": "#08143090",
                "terminal.background": "#08143010",
                "terminal.foreground": "#c1c4cb",
                "terminal.dim_foreground": "#c1c4cb",
                "terminal.bright_foreground": "#c1c4cb",
                "terminal.ansi.black": "#081430",
                "terminal.ansi.red": "#9375B5",
                "terminal.ansi.green": "#A66BA4",
                "terminal.ansi.yellow": "#DF719E",
                "terminal.ansi.blue": "#9B74C9",
                "terminal.ansi.magenta": "#7581DE",
                "terminal.ansi.cyan": "#EB8DAA",
                "terminal.ansi.white": "#c1c4cb",
                "terminal.ansi.bright_black": "#606675",
                "terminal.ansi.bright_red": "#9375B5",
                "terminal.ansi.bright_green": "#A66BA4",
                "terminal.ansi.bright_yellow": "#DF719E",
                "terminal.ansi.bright_blue": "#9B74C9",
                "terminal.ansi.bright_magenta": "#7581DE",
                "terminal.ansi.bright_cyan": "#EB8DAA",
                "terminal.ansi.bright_white": "#c1c4cb",
                "modified": "#9375B5",
                "syntax": {
                    "attribute": {
                        "color": "#9375B5"
                    },
                    "boolean": {
                      "color": "#A66BA4"
                    },
                    "comment": {
                      "color": "#606675"
                    },
                    "comment.doc": {
                      "color": "#606675"
                    },
                    "constant": {
                      "color": "#A66BA4"
                    },
                    "function": {
                      "color": "#EB8DAA"
                    },
                    "keyword": {
                      "color": "#9B74C9"
                    },
                    "number": {
                      "color": "#7581DE"
                    },
                    "operator": {
                      "color": "#9B74C9"
                    },
                    "string": {
                      "color": "#9375B5"
                    },
                    "variable": {
                      "color": "#A66BA4"
                    }
                },
                "players": [
                  {
                    "cursor": "#9375B5",
                    "background": "#081430",
                    "selection": "#9375B530"
                  }
                ]
            }
        }
    ]
}
