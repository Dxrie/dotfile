{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#12101e90",
                "editor.background": "#12101e90",
                "editor.foreground": "#c3c3c6",
                "text": "#c3c3c6",
                "text.muted": "#c3c3c670",
                "text.ignored": "#c3c3c640",
                "text.placeholder": "#c3c3c650",
                "ignored": "#c3c3c630",
                "element.hover": "#c3c3c630",
                "ghost_element.hover": "#8C789330",
                "ghost_element.selected": "#8C789330",
                "ghost_element.active": "#8C789360",
                "border": "#12101e",
                "editor.highlighted_line.background": "#8C789310",
                "editor.active_line.background": "#8C789310",
                "panel.background": "#12101e90",
                "title_bar.background": "#12101e90",
                "title_bar.inactive_background": "#12101e90",
                "status_bar.background": "#12101e90",
                "drop_target.background": "#12101e90",
                "elevated_surface.background": "#12101e",
                "toolbar.background": "#12101e90",
                "tab_bar.background": "#12101e90",
                "tab.inactive_background": "#12101e90",
                "tab.active_background": "#8C789330",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#12101e",
                "scrollbar.thumb.background": "#8C7893",
                "editor.gutter.background": "#12101e90",
                "terminal.background": "#12101e10",
                "terminal.foreground": "#c3c3c6",
                "terminal.dim_foreground": "#c3c3c6",
                "terminal.bright_foreground": "#c3c3c6",
                "terminal.ansi.black": "#12101e",
                "terminal.ansi.red": "#8C7893",
                "terminal.ansi.green": "#7C819B",
                "terminal.ansi.yellow": "#A293A9",
                "terminal.ansi.blue": "#CAA7B9",
                "terminal.ansi.magenta": "#BAB6C4",
                "terminal.ansi.cyan": "#CABAC3",
                "terminal.ansi.white": "#c3c3c6",
                "terminal.ansi.bright_black": "#5f5d71",
                "terminal.ansi.bright_red": "#8C7893",
                "terminal.ansi.bright_green": "#7C819B",
                "terminal.ansi.bright_yellow": "#A293A9",
                "terminal.ansi.bright_blue": "#CAA7B9",
                "terminal.ansi.bright_magenta": "#BAB6C4",
                "terminal.ansi.bright_cyan": "#CABAC3",
                "terminal.ansi.bright_white": "#c3c3c6",
                "modified": "#8C7893",
                "syntax": {
                    "attribute": {
                        "color": "#8C7893"
                    },
                    "boolean": {
                      "color": "#7C819B"
                    },
                    "comment": {
                      "color": "#5f5d71"
                    },
                    "comment.doc": {
                      "color": "#5f5d71"
                    },
                    "constant": {
                      "color": "#7C819B"
                    },
                    "function": {
                      "color": "#CABAC3"
                    },
                    "keyword": {
                      "color": "#CAA7B9"
                    },
                    "number": {
                      "color": "#BAB6C4"
                    },
                    "operator": {
                      "color": "#CAA7B9"
                    },
                    "string": {
                      "color": "#8C7893"
                    },
                    "variable": {
                      "color": "#7C819B"
                    }
                },
                "players": [
                  {
                    "cursor": "#8C7893",
                    "background": "#12101e",
                    "selection": "#8C789330"
                  }
                ]
            }
        }
    ]
}
